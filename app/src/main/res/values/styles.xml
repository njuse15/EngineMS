<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:divider">@drawable/divider_gray</item>
    </style>

    <style name="upload_photo_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/anim_popup_in</item>
        <item name="android:windowExitAnimation">@anim/anim_popup_out</item>
    </style>

</resources>
